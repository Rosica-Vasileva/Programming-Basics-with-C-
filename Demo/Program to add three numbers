// INPUT   OUTPUT
// 9       10
// 7
// 6

int yellowApple = int.Parse(Console.ReadLine());
int greenApple = int.Parse(Console.ReadLine());
int spoiledApple = int.Parse(Console.ReadLine());

int remainingApple = yellow + green - spoiled;
Console.WriteLine($"Total {remainingApple}");

// INPUT   OUTPUT
// 24       28
// 21
// 17
int kids4B = int.Parse(Console.ReadLine());
int kids4C = int.Parse(Console.ReadLine());
int sickids = int.Parse(Console.ReadLine());

int holiday = kids4B + kids4C - sickids;
Console.WriteLine($"Total {holiday}");
